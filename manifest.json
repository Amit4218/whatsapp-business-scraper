{
  "manifest_version": 3,
  "name": "Scrape",
  "version": "1.0.0",
  "description": "Scraping data from whatsapp business",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icons/16_img.png",
      "48": "icons/48_img.png",
      "128": "icons/128_img.png"
    }
  },
  "icons": {
    "16": "icons/16_img.png",
    "48": "icons/48_img.png",
    "128": "icons/128_img.png"
  },
  "permissions": ["activeTab", "scripting","storage"]
}
